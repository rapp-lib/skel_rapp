{
    "name": "rapp-lib/skel_rapp",
    "type": "project",
    "require": {
        "rapp-lib/rlib": "dev-master",
        "doctrine/orm": "v2.*@stable"
    },
    "autoload": {
        "psr-4": {
            "R\\App\\": "app/",
            "R\\Plugin\\": "plugins/",
            "R\\Util\\": "util/"
        }
    },
    "include-path":[
        "app/",
        "app/include/",
        "app/controller/",
        "app/list/"
    ],
    "config": {
        "preferred-install": {
            "rapp-lib/*": "source"
        }
    }
}
